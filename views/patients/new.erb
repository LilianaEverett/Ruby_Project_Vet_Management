
<form method="post" action="/patients">
  <label for="name">Name: </label>
  <input type="text" name="name" id="name">

  <label for="type">type: </label>
  <input type="text" name="type" id="type">

  <label for="db">Date of Birth: </label>
  <input type="text" name="db" id="db">

  <label for="owner_contact">Owner Contact: </label>
  <input type="text" name="owner_contact" id="owner_contact">

    <label for="vet_select">Select House:</label>
      <select name="vet_id" id="vet_select">
        <% @vets.each do |vet| %>
            <option value=<%= vet.id %>><%= vet.first_name %> <%= vet.last_name %></option>
        <% end %>
      </select>

  <label for="notes">Appointment Notes: </label>
  <input type="text" name="notes" id="notes">

  <input type="submit" value="Save">
</form>
